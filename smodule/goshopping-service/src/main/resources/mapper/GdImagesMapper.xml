<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.gw.shoping.mapper.GdImagesMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="org.gw.shoping.entity.GdImages">
        <id column="imagesId" property="imagesId"/>
        <result column="imagesurl" property="imagesurl"/>
        <result column="comdityId" property="comdityId"/>
        <result column="imageslv" property="imageslv"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        imagesId, imagesurl, comdityId, imageslv
    </sql>

    <insert id="saveImages" parameterType="list">
        insert into GoShopping.gd_images (imagesurl, comdityId, imageslv) values
        <foreach collection="list" item="item" separator=",">
            (#{item.imagesurl},#{item.comdityId},#{item.imageslv})
        </foreach>
    </insert>
</mapper>
